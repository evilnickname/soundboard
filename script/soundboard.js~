var data = {[
	sound : {
		id : 'heh',
		timing: [0, 1250],
		text: 'Heheh heh'
	},
	sound : {
		id : 'cute',
		timing : [1250, 2000],
		text: 'Isn\'t that cute'
	},
	sound : {
		id : 'wrong',
		timing : [3250, 2846],
		text : 'But it\'s wrong!'
	}
]}

var sound = new Howl({
  urls: ['_assets/cutebutwrong.mp3', '_assets/cutebutwrong.ogg'],
  sprite: {
    heh: [0, 1250],
    cute: [1250, 2000],
    wrong: [3250, 2846]
  }
});

// shoot the laser!
//sound.play('laser');
var btns = document.querySelectorAll('button');
for (var i=0, n = btns.length; i < n; i++) {
	btns[i].addEventListener('click', function() { sound.play(this.getAttribute('data-sound'))});
}

